{
  "name": "semaio/laravel-binary-uuid",
  "description": "Binary support for UUIDs in Laravel",
  "keywords": [
    "spatie",
    "laravel-binary-uuid"
  ],
  "homepage": "https://github.com/semaio/laravel-binary-uuid",
  "license": "MIT",
  "authors": [
    {
      "name": "Brent Roose",
      "email": "brent@spatie.be",
      "homepage": "https://spatie.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/queue": "^6.0|^7.0",
    "ramsey/uuid": "^3.9"
  },
  "require-dev": {
    "graham-campbell/testbench": "^5.4",
    "phpunit/phpunit": "^8.5",
    "scrutinizer/ocular": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "Spatie\\BinaryUuid\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spatie\\BinaryUuid\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Spatie\\BinaryUuid\\UuidServiceProvider"
      ]
    }
  }
}
